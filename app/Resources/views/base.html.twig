<html >
<head>
    <meta charset="UTF-8" />
    <title >{% block title %}Welcome!{% endblock %}</title >

    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    <title >Tarifs du musée du Louvre</title >

    {% block stylesheets %}
        <link rel="stylesheet" type="text/css" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" >

        <link rel="stylesheet" href="{{ asset('bundles/loremwebticketmanager/css/base.css') }}" >
        <link rel="stylesheet" href="{{ asset('bundles/loremwebticketmanager/css/layout.css') }}" >
        <link rel="stylesheet" href="{{ asset('bundles/loremwebticketmanager/css/module.css') }}" >
        <link rel="stylesheet" href="{{ asset('bundles/loremwebticketmanager/css/state.css') }}" >
        <link rel="stylesheet" href="{{ asset('bundles/loremwebticketmanager/css/theme.css') }}" >
        <link rel="stylesheet" href="{{ asset('css/base.css') }}" >
        <link rel="stylesheet" href="{{ asset('css/layout.css') }}" >
        <link rel="stylesheet" href="{{ asset('css/module.css') }}" >
        <link rel="stylesheet" href="{{ asset('css/state.css') }}" >
        <link rel="stylesheet" href="{{ asset('css/theme.css') }}" >

    {% endblock %}


</head>
<body>
<!-- Menu -->
<nav class="navbar navbar-default" >
    <div class="container" >
        <div class="navbar-header" >

            <a class="navbar-brand" href="http://www.louvre.fr/" >
                <img alt="Brand" class="logo" src="{{ asset("image/logo.png") }}" width=50 height=50 />
            </a >
            <span class="menu-titre" >Billetterie du musée du Louvre</span >

        </div >

        <div class="pull-right" >
            {# Bouton de retour à la page d'accueil #}
            <a href="http://www.louvre.fr/" class="btn btn-sm btn-default menu-bouton" >Retour à la page d'accueil</a >

            {# Selection du langage #}
            <label for="lanque" >language : </label >
            <select name="langue" id="lanque" >
                <option value="français" >Français</option >
                <option value="anglais" >English</option >
            </select >
        </div >
    </div >
</nav >

<!-- Main content -->
<div class="contenuPrincipal center-block " >
    <h1 >Réservation de billets</h1 >
    {% block body %}

    {% endblock %}

</div >


<footer >
    <div class="text-center" ><span >Copyright Musée du Louvre 2016 © Tous droits réservés</span > -
        <a href="" >Mentions légales et CGV</a ></div >
</footer >

{% block javascripts %}
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.0/jquery.js" ></script >
    <script type="text/javascript" src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.js" ></script >
    <script src="{{ asset('javascript/javascript.js') }}" ></script >
{% endblock %}
</body>
</html>
